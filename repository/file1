
int testCmp(char* g, int d, int h){
    char* h1 = "abc";
    char* h2 = "def";
    if(strcmp(g, h1) == 0) {
        h = 1;
    }else if (strcmp(g, h2) == 0){
        h = 2;
    }else {
        if(d > 1) {
            int tmp = d % 2;
            			if(tmp > 0) {
                            h = tmp - 1;
            			}else {
                            h = tmp;
            			}
            
        }
    }
    return h;
}