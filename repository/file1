
int testCmp(char* g, int h, int d){
    char* h1 = "abc";
    char* h2 = "def";
    if(strcmp(g, h1) == 0) {
        d = 1;
    }else if (strcmp(g, h2) == 0){
        d = 2;
    }else {
        if(h > 1) {
            int tmp = h % 2;
            			if(tmp > 0) {
                            d = tmp - 1;
            			}else {
                            d = tmp;
            			}
            
        }
    }
    return d;
}